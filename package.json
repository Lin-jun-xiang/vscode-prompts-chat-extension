{
  "name": "vscode-prompts-chat-extension",
  "displayName": "VSCode Prompts Chat Extension",
  "description": "An extension to connect to prompts.chat and add prompt templates for VSCode Github Copilot.",
  "version": "0.1.0",
  "publisher": "junxiang",
  "engines": {
    "vscode": "^1.60.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "promptsChat.insertPrompt",
        "title": "Prompt: Select Prompt"
      }
    ]
  },
  "activationEvents": [
    "onCommand:promptsChat.insertPrompt"
  ],
  "dependencies": {
    "axios": "^1.8.4",
    "node-fetch": "^2.7.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node-fetch": "^2.6.12",
    "@types/vscode": "^1.99.1",
    "mocha": "^11.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vscode": "^1.1.37",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1"
  }
}
